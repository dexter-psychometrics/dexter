@ARTICLE{BolsinovaMaris2016,
  AUTHOR={Bolsinova, Maria and Maris, Gunter},   
	TITLE={Can IRT Solve the Missing Data Problem in Test Equating?},      
	JOURNAL={Frontiers in Psychology},      
	VOLUME={6},      
  PAGES={1956},     
  YEAR={2016},      
	URL={https://www.frontiersin.org/article/10.3389/fpsyg.2015.01956},       
  DOI={10.3389/fpsyg.2015.01956},      
	ISSN={1664-1078},   
ABSTRACT={In this paper test equating is considered as a missing data problem. The unobserved responses of the reference population to the new test must be imputed to specify a new cutscore. The proportion of students from the reference population that would have failed the new exam and those having failed the reference exam are made approximately the same.  We investigate whether item response theory (IRT) makes it possible to identify the distribution of these missing responses and the distribution of test scores from the observed data without parametric assumptions for the ability distribution. We show that while  the score distribution is not fully identifiable, the uncertainty about the score distribution on the new test due to non-identifiability is very small. Moreover, ignoring the non-identifiability issue and assuming a normal distribution for ability may lead to bias in test equating, which we illustrate in simulated and empirical data examples.}
}	

